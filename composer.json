{
  "name": "entrepreneur-interet-general/gobelins",
  "description": "Open access website for the Mobilier National collection.",
  "keywords": [
    "GLAM",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.2",
    "artesaos/seotools": "^0.16.0",
    "basemkhirat/elasticsearch": "^1.3",
    "caouecs/laravel-lang": "~4.0",
    "fideloper/proxy": "^4.0",
    "flynsarmy/csv-seeder": "^1.0",
    "folklore/image": "0.3.28",
    "guzzlehttp/guzzle": "^6.3",
    "kalnoy/nestedset": "^5.0",
    "laravel/framework": "^6.0",
    "laravel/helpers": "^1.1",
    "laravel/scout": "^7.2",
    "laravel/tinker": "^2.0",
    "laravelium/sitemap": "6.0.*",
    "league/flysystem": "^1.0",
    "league/flysystem-memory": "^1.0",
    "nothingworks/blade-svg": "^0.3",
    "seld/signal-handler": "^1.1",
    "spatie/laravel-image-optimizer": "^1.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "composer/composer": "^1.9",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^2.0",
    "phpunit/phpunit": "^7.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/silasrm/laravel-image"
    }
  ],
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "folklore/image"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
